{
  "name": "vesium-lib",
  "type": "module",
  "version": "1.0.1-beta.36",
  "private": true,
  "description": "Vue component and composition-api library for Cesium.",
  "license": "MIT",
  "homepage": "https://github.com/GeoVueJS/vesium",
  "repository": {
    "url": "https://github.com/GeoVueJS/vesium"
  },
  "bugs": {
    "url": "https://github.com/GeoVueJS/vesium/issues"
  },
  "keywords": [
    "vue",
    "cesium",
    "vueuse",
    "hooks",
    "composition-api"
  ],
  "scripts": {
    "docs:dev": "vitepress dev packages --host",
    "docs:build": "npm run build:types && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "build:types": "vue-tsc -p ./tsconfig.build.json --emitDeclarationOnly --declarationMap false --strict false",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "taze": "taze -w -r major minor patch",
    "release": "bumpp -r",
    "==========[CI/CD]===========": "",
    "build:ci": "tsx ./internals/build",
    "publish:ci": "tsx ./scripts/publish"
  },
  "dependencies": {
    "@unocss/reset": "^66.1.1",
    "@vesium/core": "workspace:*",
    "@vesium/shared": "workspace:*",
    "@vueuse/core": "^13.1.0",
    "cesium": "^1.129.0",
    "pinia": "^3.0.2",
    "vesium": "workspace:*",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/tabler": "^1.2.17",
    "@iconify/utils": "^2.3.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^22.15.17",
    "@unocss/eslint-plugin": "^66.1.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^10.1.0",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-plugin-format": "^1.0.1",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "local-pkg": "^1.1.1",
    "magic-string": "^0.30.17",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "npm-run-all2": "^8.0.1",
    "pnpm": "^10.10.0",
    "prettier": "^3.5.3",
    "rollup": "^4.40.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "sass": "^1.88.0",
    "start-server-and-test": "^2.0.11",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.0",
    "svgo": "^3.3.2",
    "taze": "^19.1.0",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "5.8.3",
    "unocss": "^66.1.1",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-cesium": "^2.1.1",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitepress": "^1.6.3",
    "vitepress-sidebar": "^1.31.1",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  }
}
